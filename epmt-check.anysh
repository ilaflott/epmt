rm -rf ./1 /tmp/epmt/1
epmt -h >/dev/null   # help path 1
epmt help >/dev/null # help path 2
epmt -j1 start           # Generate prolog
epmt -j1 source          # Print shell command
epmt -j1 run sleep 1     # Run command, if no papiex just run command silently
epmt -j1 stop            # Generate epilog and append
epmt -j1 dump >/dev/null # Parse/print job_metadata
epmt -j1 stage           # Move to medium term storage
epmt submit ./1         # Submit from staged storage
